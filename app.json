{
  "expo": {
    "name": "wett",
    "slug": "wett",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wett",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shade437.wett",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Your location is used to show local weather conditions",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Your location is used to provide weather alerts",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#1E88E5"
      },
      "package": "com.shade437.wett",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "FOREGROUND_SERVICE",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-location"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "experiments": {
      "typedRoutes": true,
      "turboModules": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9a512005-462e-4f2f-97b8-a7262de7a80b"
      }
    }
  }
}
